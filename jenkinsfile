node{
    stage("copy from git") {
        //git clone
        git branch: 'main', url: 'https://github.com/Srinath246/spring-petclinic.git'
    }
    stage(maven package) {
        //mvn package
        sh 'mvn package'
    }
    stage("archival") {
        //archival archifact
        artifacts: 'target/*.jar'
    }
}